!!!145410.cpp!!!	setType(in type : str, in s : str) : void
  UmlTypeSpec t;
  
  t.explicit_type = type;
  set_ReturnType(t);
 
  static const QCString sep = " \t\n\r";
  QCString d;
  int typeindex;
  int nameindex;
  
  d = cppDecl();
  if (((typeindex = d.find("${type}")) != -1) &&
      ((nameindex = d.find(" ${name}", typeindex + 7)) != -1)) {
    while (sep.find(d[nameindex - 1]) != -1)
      nameindex -= 1;
    d.replace(typeindex, nameindex - typeindex, s);
    set_CppDecl(d);
  }

  if (! isAbstract()) {
    d = cppDef();
    if (((typeindex = d.find("${type}")) != -1) &&
	((nameindex = d.find(" ${class}", typeindex + 7)) != -1)) {
      while (sep.find(d[nameindex - 1]) != -1)
	nameindex -= 1;
      d.replace(typeindex, nameindex - typeindex, s);
      set_CppDef(d);
    }
  }
