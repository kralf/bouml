!!!145538.cpp!!!	addParam(in rank : int, in dir : aDirection, in name : str, inout type : UmlClass) : void
  UmlParameter p;
    
  p.dir = dir;
  p.type.type = type;
  p.name = name;
      
  if (params().count() <= (unsigned) rank)
    addParameter(rank, p);
  else
    replaceParameter(rank, p);
!!!145666.cpp!!!	setParams(in s : str) : void
  QCString d;
  int index;
  
  d = cppDecl();
  if (((index = d.find("${(}")) != -1) &&
      (d.mid(index + 4, 4) == "${)}")) {
    d.insert(d.find("${(}") + 4, s);
    set_CppDecl(d);
  }

  d = cppDef();
  if (((index = d.find("${(}")) != -1) &&
